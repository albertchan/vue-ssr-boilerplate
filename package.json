    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-import": "^1.16.0",
    "file-loader": "^0.9.0",
    "node-sass": "~3.13.0",
    "rimraf": "^2.5.4",
    "sass-loader": "~4.0.0",
    "style-loader": "~0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.2",
    "sw-precache-webpack-plugin": "^0.6.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.9.3",
    "webpack": "^2.1.0-beta.26",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
